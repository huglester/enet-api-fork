{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2018",
        "lib": [
          "ES2015",
          "ES2016",
          "ES2017",
          "ES2018"
        ],
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "allowJs": true,
        "outDir": "dist"
    },
    "include": [
        "src/*"
    ],
    "exclude": []
}

